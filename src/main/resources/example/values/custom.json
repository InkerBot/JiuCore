// use UTF-8
{
	// value name, use string, can't lose
	// 数值默认名称，使用字符串定义，不可无
	"custom": [
		"san"
	],
	
	// value display name, can lose
	// 数值显示的名称，可无
	"language": {
		// the language name, can lose other language
		// 对应的语言文件名称，可没有具体的某个语言
		"zh_cn": {
			"san": "理智"
		},
		"en_us": {
			"san": "Sanity"
		}
	},
	
	// Initial value on add custom value, use number, can lose
	// 数值初始值，使用数字定义，可无
	"default": {
		"san": 1000000000000
	}
}
